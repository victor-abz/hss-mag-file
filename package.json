{
	"name": "hssmag-automator",
	"version": "0.1.0",
	"description": "A system to manage file in order to speed up work",
	"main": "public/assets/js/main.js",
	"scripts": {
		"py-install": "pip install -r requirements.txt",
		"py-build": "pyinstaller --workpath=engine/build --distpath=engine/dist api.spec",
		"electron-dev": "electron .",
		"electron-build:win": "electron-builder --config electron-builder.config.json  --win --x64",
		"electron-build:mac": "electron-builder --config electron-builder.config.json   --mac"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/victor-abz/hss-mag-file.git"
	},
	"author": "Victor Abizeyimana",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/victor-abz/hss-mag-file/issues"
	},
	"homepage": "https://github.com/victor-abz/hss-mag-file#readme",
	"devDependencies": {
		"electron": "^13.1.2",
		"electron-builder": "^22.11.7",
		"electron-reloader": "^1.2.1",
		"python-shell": "^3.0.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"electron-is-dev": "^1.2.0"
	}
}
